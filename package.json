    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.11",
    "nodemailer": "^6.2.1",
    "nodemailer-express-handlebars": "^3.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "phaser": "^3.11.0",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
